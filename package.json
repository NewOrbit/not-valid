{
  "name": "@neworbit/validation",
  "version": "1.1.0",
  "description": "NewOrbit validation",
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "scripts": {
    "prepublish": "npm run review && npm run build",
    "build": "tsc",
    "lint:src": "tslint -c tslint.json src/**/*.ts",
    "lint:test": "tslint -c tslint.json test/**/*.test.ts",
    "lint": "npm run lint:src && npm run lint:test",
    "test": "npm run build && alsatian \"test/**/*.test.js\"",
    "review": "npm run test && npm run lint"
  },
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts"
  ],
  "keywords": [
    "validation"
  ],
  "license": "ISC",
  "author": "NewOrbit Ltd",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NewOrbit/NewOrbit.validation.git"
  },
  "bugs": {
    "url": "https://github.com/NewOrbit/NewOrbit.validation/issues"
  },
  "homepage": "https://github.com/NewOrbit/NewOrbit.validation#readme",
  "dependencies": {},
  "devDependencies": {
    "@neworbit/tslint-config": "0.0.6",
    "alsatian": "^2.0.0",
    "tslint": "^5.2.0",
    "typescript": "^2.4.2"
  }
}
